{
  "imports": {
    "mysql": "https://deno.land/x/mysql@v2.12.1/mod.ts",
    "postgres": "jsr:@db/postgres@0.19",
    "supabase": "https://esm.sh/@supabase/supabase-js@2.39.0",
    "uuid": "https://deno.land/x/uuid@v0.1.2/mod.ts",
    "media_types/": "https://deno.land/x/media_types@v3.0.4/",
    "dotenv": "https://deno.land/std@0.224.0/dotenv/mod.ts",
    "oak": "https://deno.land/x/oak@v17.1.5/mod.ts",
    "zod": "https://esm.sh/zod@3.22.4",
    "jose": "https://deno.land/x/jose@v6.0.11/index.ts",
    "djwt": "https://deno.land/x/djwt@v3.0.2/mod.ts",
    "http": "https://deno.land/std@0.224.0/http/mod.ts",
    "test": "https://deno.land/std@0.203.0/testing/asserts.ts",
    "oakCors": "https://deno.land/x/cors@v1.2.2/mod.ts",
    "csv": "https://deno.land/std@0.224.0/csv/mod.ts",
    "log": "https://deno.land/std@0.224.0/log/mod.ts",
    "xlsx": "npm:xlsx",
    "postgres-pool": "https://deno.land/x/postgres@v0.19.3/mod.ts"
  },
  "entry": "src/main.ts",
  "tasks": {
    "start": "deno run -A src/main.ts",
    "dev": "deno run --allow-net --allow-env --allow-read --allow-sys --watch src/main.ts",
    "testConection": "deno run --allow-env --allow-net --allow-read src/test/pruvaposgrst.ts",
    "test": "deno test --allow-net --allow-env --allow-read --allow-write --unstable --coverage=coverage --parallel --allow-hrtime src/test/test_connection.ts",
    "testPruebasCSV": "deno run --allow-env --allow-net --allow-read src/Utils/PruevasCSV.ts"
  },
  "deploy": {
    "project": "tu-proyecto-id",
    "entrypoint": "src/main.ts",
    "include": ["src/"],
    "exclude": ["node_modules/", ".git/"]
  }
}
